<config xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
  <authgroups>
    <group>
      <name>my-group</name>
      <default-map>
        <remote-name>admin</remote-name>
        <remote-password>admin</remote-password>
      </default-map>
    </group>
  </authgroups>
  <device>
    <name>router0</name>
    <address>127.0.0.1</address>
    <port>12022</port>
    <authgroup>my-group</authgroup>
    <device-type>
      <netconf>
        <ned-id>ned:netconf</ned-id>
      </netconf>
    </device-type>
    <trace>raw</trace>
    <state>
      <admin-state>unlocked</admin-state>
    </state>
  </device>
  <device>
    <name>router1</name>
    <address>127.0.0.1</address>
    <port>12032</port>
    <authgroup>my-group</authgroup>
    <device-type>
      <netconf>
        <ned-id>ned:netconf</ned-id>
      </netconf>
    </device-type>
    <trace>raw</trace>
    <state>
      <admin-state>unlocked</admin-state>
    </state>
  </device>
  <device>
    <name>router2</name>
    <address>127.0.0.1</address>
    <port>12042</port>
    <authgroup>my-group</authgroup>
    <device-type>
      <netconf>
        <ned-id>ned:netconf</ned-id>
      </netconf>
    </device-type>
    <trace>raw</trace>
    <state>
      <admin-state>unlocked</admin-state>
    </state>
  </device>
  </devices>
</config>
